{#
/**
 * @file
 * Theme override to display a component set.
 * @override modules/contrib/paragraphs/templates/paragraph.html.twig
 *
 * Fields available:
 * field_title - Title of this component
 * field_question_answer_set - Paragraph field of Questions (containing Paragraph field of Answers) - @see paragraph--question.html.twig
 */
#}
{%
set classes = [
'paragraph',
'paragraph--type--' ~ paragraph.bundle|clean_class,
view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
]
%}
<div{{ attributes.addClass(classes) }}>
    {{ content }}
</div>
