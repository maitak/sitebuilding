{#
/**
 * @file
 * Theme override to display a question set.
 * @override modules/contrib/paragraphs/templates/paragraph.html.twig
 *
 * Fields available:
 * field_question - Text of this question
 * field_answer_set - Paragraph field of Answers - @see paragraph--answer.html.twig
 */
#}
{%
set classes = [
'paragraph',
'paragraph--type--' ~ paragraph.bundle|clean_class,
view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
]
%}
<div{{ attributes.addClass(classes) }}>
    {{ content }}
</div>
